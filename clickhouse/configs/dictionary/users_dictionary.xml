<yandex>
<!--USERS-->
  <dictionary>
    <name>users</name>

  <source>
    <mysql>
        <host>mysql</host>
          <port>3306</port>
          <user>root</user>
          <password>secret</password>
        <db>tortuga</db>
        <table>users</table>
    </mysql>
  </source>

      <layout>
        <flat />
      </layout>

      <structure>
        <id>
            <name>id</name>
        </id>
        <attribute>
          <name>name</name>
          <type>String</type>
          <null_value></null_value>
          <injective>false</injective>
        </attribute>
        <attribute>
          <name>tb_id</name>
          <type>UInt64</type>
          <null_value></null_value>
          <injective>false</injective>
        </attribute>
        <attribute>
          <name>gosb_id</name>
          <type>UInt64</type>
          <null_value></null_value>
          <injective>false</injective>
        </attribute>
      </structure>

      <lifetime>3600</lifetime>
  </dictionary>
<!--USERS_EMAIL-->
  <dictionary>
      <name>users_email</name>

      <source>
        <mysql>
           <host>mysql</host>
          <port>3306</port>
          <user>root</user>
          <password>secret</password>
            <db>tortuga</db>
            <table>users_email</table>
        </mysql>
  </source>

      <layout>
        <complex_key_hashed />
      </layout>

      <structure>
        <key>
          <attribute>
            <name>email</name>
            <type>String</type>
          </attribute>
        </key>
        <attribute>
          <name>user_id</name>
          <type>UInt64</type>
          <null_value></null_value>
          <injective>false</injective>
        </attribute>
      </structure>

      <lifetime>3600</lifetime>
  </dictionary>
</yandex>
